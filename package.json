{
  "name": "@critocrito/namefn",
  "description": "Set the name property of a function.",
  "license": "GPL-3.0",
  "author": "Christo <christo@cryptodrunks.net>",
  "homepage": "https://github.com/critocrito/namefn#readme",
  "version": "0.1.4",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn flow:libdefs && yarn lint && yarn test && yarn compile",
    "clean": "rimraf dist",
    "precompile": "yarn clean",
    "compile": "babel -s -D -d dist src",
    "postcompile": "yarn flow:gen",
    "compile:watch": "yarn compile -w",
    "fix": "yarn lint --fix",
    "flow:coverage": "flow-coverage-report --config ./.flowcoverage",
    "flow:gen": "flow gen-flow-files --out-dir dist src",
    "flow:libdefs": "flow-libdefs update",
    "lint": "eslint test src",
    "prepublishOnly": "yarn build",
    "prerelease": "yarn build",
    "release": "standard-version -i CHANGELOG.md -m \"chore(release): Bumped to version %s.\" --sign",
    "test": "ava",
    "test:coverage": "nyc yarn test",
    "test:watch": "yarn test -w"
  },
  "devDependencies": {
    "@build-chores/babel-preset": "0.7.0",
    "@build-chores/commitlint": "0.7.0",
    "@build-chores/eslint-config-lint": "0.7.0",
    "@build-chores/staged": "0.7.0",
    "@build-chores/test": "0.7.0",
    "ava": "1.0.0-beta.8",
    "ava-jsverify": "^1.0.3",
    "coveralls": "^3.0.0",
    "husky": "^1.1.1",
    "jsverify": "^0.8.3",
    "rimraf": "^2.6.2",
    "standard-version": "^4.3.0"
  },
  "keywords": [
    "fp",
    "functional programming",
    "functions",
    "repl",
    "utility"
  ],
  "ava": {
    "sources": [
      "src/**/*.js"
    ],
    "require": [
      "@build-chores/test"
    ]
  }
}
